# BirdSphere Frontend Environment Configuration

# Development Server Port
# Create React App will use this port by default
PORT=3005

# Backend API URL (development)
# Note: In production, this would be your production API URL
REACT_APP_API_URL=http://localhost:3000

# Environment
NODE_ENV=development

# Browser Configuration
BROWSER=none
GENERATE_SOURCEMAP=true

# Build Configuration
PUBLIC_URL=
BUILD_PATH=build

# Development Server Options
HOST=localhost
HTTPS=false
WDS_SOCKET_HOST=
WDS_SOCKET_PATH=
WDS_SOCKET_PORT=

# ESLint Configuration
ESLINT_NO_DEV_ERRORS=false
DISABLE_ESLINT_PLUGIN=false

# TypeScript Configuration
TSC_COMPILE_ON_ERROR=false
SKIP_PREFLIGHT_CHECK=false

# Fast Refresh
FAST_REFRESH=true